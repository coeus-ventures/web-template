{
  "name": "agents",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080 --turbopack",
    "dev:start": "pm2 start \"bun run dev\" --log ./logs/dev.log --name dev && pm2 start \"bun run drizzle:gateway\" --log ./logs/gateway.log --name gateway",
    "dev:logs": "pm2 logs dev",
    "dev:restart": "pm2 restart dev",
    "dev:test": "USE_TEST_DB=true next dev --turbopack -p 3001 > logs/test.log 2>&1 &",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "NODE_ENV=test vitest run --reporter=verbose",
    "spec": "NODE_ENV=test playwright test --reporter=list",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "bun db/scripts/seed.ts",
    "db:clean": "bun db/scripts/clean.ts",
    "db:squash": "bun db/scripts/squash.ts",
    "db:reset": "bun db/scripts/clean.ts && bun run db:push",
    "drizzle:gateway": "bun run scripts/drizzle-gateway/gateway-starter.ts",
    "drizzle:gateway:dev": "bun --watch scripts/drizzle-gateway/gateway-starter.ts",
    "shadcn:add": "npx shadcn@latest add"
  },
  "dependencies": {
