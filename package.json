{
  "name": "agents",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "NODE_ENV=test vitest run --reporter=verbose",
    "spec": "NODE_ENV=test playwright test --reporter=list",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "bun db/scripts/seed.ts",
    "db:clean": "bun db/scripts/clean.ts",
    "db:reset": "bun db/scripts/clean.ts && bun run db:push",
    "db:squash": "bun db/scripts/squash.ts --all && bun run db:generate"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.57",
    "@libsql/client": "^0.15.15",
    "@radix-ui/react-slot": "^1.2.3",
    "ai": "^5.0.82",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "glob": "^11.0.3",
    "lucide-react": "^0.546.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "jsdom": "^27.0.1",
    "playwright": "^1.56.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.2"
  }
}
